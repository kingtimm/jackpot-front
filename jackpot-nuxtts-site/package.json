{
  "name": "jackpot-nuxtts-site",
  "description": "Nuxt.js TypeScript project",
  "version": "2.9.2",
  "private": true,
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.1.6",
    "@nuxtjs/axios": "5.6.0",
    "fibers": "^4.0.1",
    "lodash": "latest",
    "node-sass": "^4.12.0",
    "nuxt": "2.9.2",
    "nuxt-buefy": "^0.3.15",
    "nuxt-property-decorator": "2.4.0",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "ts-money": "^0.4.6",
    "vuex-class": "0.3.2"
  },
  "scripts": {
    "dev": "nuxt-ts",
    "test:unit": "jest",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "0.2.9",
    "@types/jest": "^23.3.10",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.9",
    "@vue/cli-service": "^3.0.0-rc.9",
    "@vue/test-utils": "^1.0.0-beta.22",
    "jest": "^23.4.2",
    "ts-jest": "^23.0.1",
    "typescript": "^3.6.3",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$"
  }
}
